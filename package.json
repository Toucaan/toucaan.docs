{
  "name": "toucaan-docs",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "doc": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "ci": "yarn lint && yarn format:diff",
    "lint": "eslint --cache \"**/*.js\" && stylelint \"**/*.css\"",
    "format": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "format:diff": "prettier --config .prettierrc --list-different \"**/*.{js,jsx,ts,tsx,md,mdx}\""
  },
  "dependencies": {
    "@docusaurus/core": "^3.1.0",
    "@docusaurus/plugin-ideal-image": "^3.1.0",
    "@docusaurus/plugin-pwa": "^3.1.0",
    "@docusaurus/plugin-sitemap": "^3.1.0",
    "@docusaurus/preset-classic": "^3.1.0",
    "@docusaurus/theme-mermaid": "^3.1.0",
    "@easyops-cn/docusaurus-search-local": "^0.33.5",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^1.2.1",
    "mermaid": "9.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@tsconfig/docusaurus": "^1.0.6",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.1",
    "typescript": "^4.8.4",
    "@docusaurus/module-type-aliases": "3.0.0",
    "@docusaurus/types": "3.0.0"    
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
